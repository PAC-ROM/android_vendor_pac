<property name="Phablet UI" active="true" image="/system/etc/paranoid/preferences/images/phablet.png" color="#f4b122">

        <description><![CDATA[
            This will put your device into 320 DPI/600p Phablet UI, all of your apps will be changed.
        ]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="320"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="600"/>
        <setting type="Hybrid" target="com.android.systemui.navbar.dpi" value="80"/>
        <setting type="Hybrid" target="com.android.systemui.statusbar.dpi" value="60"/>
        <setting type="Hybrid" target="%system_default_layout" value="600"/>
        <setting type="Hybrid" target="%system_default_dpi" value="320"/>
        <setting type="Hybrid" target="%user_default_layout" value="600"/>
        <setting type="Hybrid" target="%user_default_dpi" value="320"/>
        <setting type="Android" target="status_bar_show_clock" value="1"/>
        <setting type="Android" target="max_notification_icons" value="5"/>

</property>
