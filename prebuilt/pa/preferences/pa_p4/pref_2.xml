<property name="Phone UI" active="true" image="/system/etc/paranoid/preferences/images/phone.png" color="#238fcd">

        <description><![CDATA[160 DPI Phone UI<br/>No apps changed]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="360"/>
        <setting type="Hybrid" target="com.android.settings.dpi" value="160"/>
        <setting type="Hybrid" target="com.android.settings.layout" value="360"/>
        <setting type="Hybrid" target="com.android.systemui.navbar.dpi" value="100"/>
        <setting type="Hybrid" target="com.android.systemui.statusbar.dpi" value="100"/>
        <setting type="Hybrid" target="%system_default_layout" value="0"/>
        <setting type="Hybrid" target="%system_default_dpi" value="0"/>
        <setting type="Android" target="status_bar_show_clock" value="1"/>
        <setting type="Android" target="max_notification_icons" value="5"/>

</property>
