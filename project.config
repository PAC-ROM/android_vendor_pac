[project]
	description = PAC-Roms vendor.
[access "refs/*"]
	read = group Administrators
[contributor-agreement "Code of Conduct"]
	description = If you are contributing code AND hoping to be a member of the PAC organization, you must also agree to and uphold the code of conduct.
	agreementUrl = static/codeofconduct.html
	autoVerify = group Contributors Agreement
	accepted = group Contributors Agreement
[receive]
	requireContributorAgreement = true
	requireSignedOffBy = false
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/for/refs/*"]
	push = group Registered Users
	pushMerge = group Registered Users
